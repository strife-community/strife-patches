	<!-- Core Templates -->	
	<include file="/ui/shared/templates_elements_simple.package"/>
	
	<template name="ToolTip">
		<panel noclick="1" name="TipChannelOffset" valign="bottom">
			<panel name="{id}_tooltip" align="center" valign="bottom" width="40h" grow="1" regrow="1" noclick="1" passivechildren="1" y="-17h" visible="0">
				<panel>
					<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.75h" color="0 0 0 0.5" x="0.35h" y="0.35h"/>
					<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.75h" color="#151515"/>
					<frame texture="/ui/elements:roundframe" borderthickness="0.75h" color="#292929"/>
				</panel>
				<panel grow="1" regrow="1" growinvis="0" width="-1h" align="center" float="bottom" padding="0.5h">
					<panel height="0" sticky="1" stickytoinvis="0"/>
					<panel height="3h" sticky="1" stickytoinvis="0" >
						<panel width="100@" height="100%">
							<image name="{id}_tipIcon" texture="/heroes/legionnaire/icon.tga"/>
							<frame texture="/ui/elements:rounded_corner_white" borderthickness="0.75h"  color="#151515"/>
							<frame texture="/ui/elements:roundframe" borderthickness="0.75h" color="black"/>
						</panel>
						<label name="{id}_tipName" x="115@" textvalign="center" color="#FFCC00" font="maindyn_16" shadow="1" content="abilitytip_name" shadowoffset="2"/>
						<panel align="right" height="100%" grow="1">
							<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.5h" color="#0B0B0B"/>
							<frame texture="/ui/elements:roundframe" borderthickness="0.5h" color="black"/>
							<panel>
								<frame texture="/ui/elements:roundframe" borderthickness="0.5h" color="#1A1A1A" width="+2" height="+2" align="center" valign="center"/>
							</panel>
							<panel grow="1" regrow="1" float="right" height="100%">
								<label name="{id}_tipValue" textvalign="center" fitx="1" fitxpadding="1h" textalign="center" font="maindyn_14" shadow="1" style="whiteTextColor" content="115"/>
							</panel>
						</panel>
					</panel>
					<panel width="100%" grow="1" regrow="1" sticky="1" stickytoinvis="0" >
						<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.5h" color="#0B0B0B"/>
						<frame texture="/ui/elements:roundframe" borderthickness="0.5h" color="black"/>
						<panel>
							<frame texture="/ui/elements:roundframe" borderthickness="0.5h" color="#1A1A1A" width="+2" height="+2" align="center" valign="center"/>
						</panel>
						<label name="{id}_tipDescription" align="center" width="-1h" textvalign="center" fity="1" fitypadding="1h" wrap="1" font="subdyn_10" style="whiteTextColor" shadow="1" content="temp_abilitytip_desc"/>
					</panel>
					<panel height="0" sticky="1" stickytoinvis="0"/>
					<panel height="0"/>
				</panel>
			</panel>
		</panel>
	</template>
	
	<template name="key">
		<panel name="{id}" group="{group}" y="{y}" x="{x}" width="{width=3h}" height="{height=3h}" align="{align=left}" valign="{valign=top}" color="invisible" interactive="0" visible="{visible=1}">
			<panel name="{id}Body" align="center" valign="center" noclick="1">
				<frame name="{id}Backer" texture="/ui/elements:key" borderthickness="0.8h" noclick="1"/>
				<frame name="{id}Highlight" style="gameInventoryKeyHighlight"/>
				<label name="{id}Label" font="{font=maindyn_10}" textalign="center" textvalign="bottom" outline="1" outlinecolor="#DDDDDD" x="{labelXOffset}" y="{labelYOffset=-0.3h}" color="#333333" content="{keybind}" noclick="1"
					onshowlua="{onrefreshlua}"
					onrefreshlua="{onrefreshlua}"
					oneventlua="{oneventlua}"
				/>
			</panel>
			
			<button name="{id}ButtonTip" hmargin="5@" vmargin="5%" noclick="0" interactive="0" color="invisible"/>
			<button name="{id}Button" hmargin="5@" vmargin="5%" noclick="0" interactive="0" color="invisible"
				onloadlua="libButton2.initializeButton(self, '{id}', 'key')"
			/>
			<panel name="{id}Disabled" align="center" valign="center" noclick="1" visible="0">
				<frame texture="/ui/elements:key" borderthickness="0.8h" color="0.5 0.5 0.5" noclick="1"/>
			</panel>
		</panel>
	</template>
	
	<template name="simpleButtonKey">
		<panel name="{id}Container" width="{width=100%}" height="{height=100%}" x="{x}" y="{y}" visible="{visible=1}" align="{align}" valign="{valign}" onloadlua="{onloadlua}" noclick="1" sticky="{sticky}" stickytoinvis="{stickytoinvis=1}" interactive="{interactive=0}">
			<button color="invisible" visible="{visible=1}" name="{id}" interactive="0"
				onloadlua="libButton2.initializeButton(self, '{id}', 'standardButton2')"
			/>
			
			<panel name="{id}Body" align="center" valign="center" noclick="1" passivechildren="1">
				<frame name="{id}Background" texture="/ui/shared/frames/stnd_btn_up.tga" borderthickness="{borderthickness=0.6h}"/>
				<label name="{id}Label" width="-90@" height="100%" x="{labelX}" y="{labelY=-0.2h}" textalign="center" textvalign="center" style="labelBase" font="{font=maindyn_10}" color="{labelcolor=white}" content="{label=temp_button}"/>
			</panel>
			
			<instance id="{id}Key" name="key" align="right" width="90@" height="85%" valign="center" x="-0.35h"
				font="{keyFont=maindyn_10}"
				onrefreshlua="{onrefreshlua}"
				oneventlua="{onrefreshlua}"
			/>
		</panel>
	</template>
	
	<template name="framed_mini_button_state">
		<image texture="/ui/shared/textures/mini_button_stone.jpg" align="center" valign="center" noclick="1" color="{color}"/>
		<image name="{id}_icon{type}" texture="{texture}" width="90%" height="90%" align="center" valign="center" noclick="1" />
		
		<!-- Cooldown - here so that it appears under the borders -->
		<piegraph name="{id}CooldownPie{type}" style="inventoryCooldownPie" />
		
		<image texture="/ui/shared/textures/mini_button_stone.tga" align="center" valign="center" noclick="1" color="{color}"/>
	</template>
	
	<template name="framed_mini_button">
		<panel width="{width=3h}" height="{height=3h}" x="{x}" y="{y}" align="{align}" valign="{valign}" color="invisible" interactive="0" noclick="0">
			<button name="{id}" color="invisible" interactive="0" noclick="0" onmouseoverlua="{onmouseoverlua}" onmouseoutlua="{onmouseoutlua}">
				<widgetstate statename="up">
					<instance name="framed_mini_button_state" id="{id}" type="Up" texture="{texture}" color="0.8 0.8 0.8 1" />
				</widgetstate>
				<widgetstate statename="over">
					<instance name="framed_mini_button_state" id="{id}" type="Over" texture="{texture}" color="1 1 1 1" />
					<image name="{id}Gloss" style="abilityGloss" color="1 1 1 0.3" />
				</widgetstate>
				<widgetstate statename="down" x="1" y="1">
					<instance name="framed_mini_button_state" id="{id}" type="Down" texture="{texture}" color="0.6 0.6 0.6 1" />
				</widgetstate>
				<panel name="{id}CooldownPie" color="0 0 0 0.65" visible="0" noclick="true"  />
				<label name="{id}Cooldown" visible="0" style="inventoryCooldownLabel" content="0" font="subdyn_11"/>
			</button>	
			
			<panel visible="{hasHotKey=1}" noclick="1">
				<instance align="center" name="key" id="{id}MiniButton" group="{tabShow}" width="{keywidth=-0.8h}" height="{keyheight=-0.8h}" y="{keyy=-3.5h}" color="red" visible="{bindShow}" labelYOffset="-0.7h" keybind="{keybind=?}" 
					font="{keyFont=maindyn_10}"
					onrefreshlua="{onrefreshlua}"
					oneventlua="{onrefreshlua}"
					/>
			</panel>
		</panel>
	</template>
	
	<template name="gameCooldownSquare">
		<panel name="gameCooldownSquare{id}" width="3.5h" height="3.5h" noclick="0">
