		<panel width="3h" noclick="1" align="left">
			<panel style="gameXPLeft">
				<image style="gameXPLeftDecal" color="{color}"/>
				<panel style="gameXPTextContainer">
					<label style="gameLevelTotal" content="game_scores_label_lvl_caps" color="{color1}" outlinecolor="{outlinecolor1}" />
					<label name="{levelName}" style="gameLevelCurrent" content="15"  color="{color2}" outlinecolor="{outlinecolor2}"/>
				</panel>
			</panel>
		</panel>
	</template>
	
	<template name="gameXPContainer_right">		
		<panel width="0.1h" noclick="1" align="right">
			<panel style="gameXPRight">
				<image style="gameXPRightDecal" color="{color}"/>
			</panel>
		</panel>
	</template>
	
	<template name="gameXPContainer">	
		<panel x="2h" width="-3.4h" noclick="1" align="center">
			<panel style="gameXPFillContainer" color="1 1 1 {alpha2}" >
				<frame style="generalShadow" color="0 0 0 0.3" bordercolor="0 0 0 0.3"/>
				
				<panel name="{fillName}_gained" style="gameXPFill" width="{width=0%}" visible="0">
					<image style="gameXPFillDecal" texture="/ui/shared/textures/xp_bar_gained.tga" color="1 1 1 {alpha2}" />
				</panel>
				
				<panel name="{fillName}_gainedEdge" style="gameXPEnd" visible="0">
					<!-- anything in this panel will appear at the END of the gained fill bar -->
					<image style="gameXPFillGained_glowEnd" color="1 1 1 {alpha2}"/>
				</panel>
				
				<panel name="{fillName}" style="gameXPFill" width="{width=0%}" >
					<image style="gameXPFillDecal" color="1 1 1 {alpha2}" />
				</panel>
				
				<panel style="gameXPEnd">
					<!-- anything in this panel will appear at the END of the fill bar -->
					<image style="gameXPFillDecal_glowEnd" color="1 1 1 {alpha2}"/>
				</panel>
			</panel>
			
			<panel style="gameXPMax" visible="{max}">
				<image x="-0.5h" y="0.3h" texture="/ui/shared/textures/decal_max.tga" width="2200@" height="550%" align="center" valign="center" noclick="1" />
				<label style="gameLevelCurrent" align="center" textvalign="center" content="game_scores_label_lvl_max" color="#f55bb2" outlinecolor="black"/>
			</panel>
		</panel>
		
		<instance name="gameXPContainer_left" levelName="{levelName}" color="1 1 1 {alpha}" color1="0.78 0.1 0.49 {color1alpha}" color2="0.96 0.36 0.7 {color2alpha}" outlinecolor1="0 0 0 {outline1alpha}" outlinecolor2="0 0 0 {outline2alpha}"/>
		<instance name="gameXPContainer_right" color="1 1 1 {alpha}"/>
	</template>
	
	<!-- Ability Templates -->
	<template name="gameAbilityTick">
		<panel name="gameAbility{id}_tick{count}Container" style="gameAbilityIconTick_none" sticky="1" stickytoinvis="1">
			<image width="175%" height="190@" texture="/ui/shared/textures/pip_base.tga" align="center" valign="bottom" noclick="1" />
			
			<panel name="gameAbility{id}_tick{count}" style="gameAbilityIconTick_complete">
				<image width="175%" height="190@" texture="/ui/shared/textures/pip_active.tga" align="center" valign="bottom" noclick="1" />
			</panel>
		</panel>
	</template>
	
	<template name="gameAbilityLevel">
		<frame style="gameAbilityLevel_none">
			<image name="gameAbility{id}_leveled" texture="/ui/shared/textures/ability_leveled.tga" width="90@" height="130%" align="center" valign="center" noclick="1"/>
		</frame>
	</template>
	
	<template name="gameAbilityHeroInfo" >
		<instance name="gameAbilityLevel" id="{id}_1"/>
		<instance name="gameAbilityLevel" id="{id}_2"/>
		<instance name="gameAbilityLevel" id="{id}_3"/>
		<instance name="gameAbilityLevel" id="{id}_4"/>
	</template>
	
	<template name="gameAbilityPetInfo">
		<instance name="framed_mini_button" id="gamePetPassiveButton" height="107%" width="107@"
		
			onmouseoverlua="
				gamePetTipShow()
				UpdateCursor(widget, true, { canLeftClick = true})
			"
			
			onmouseoutlua="
				gamePetTipHide()
				UpdateCursor(widget, false, { canLeftClick = true})
			"
			
			/>
	</template>
	
	<template name="gameAbilityTemplate">
		<panel name="gameAbility{id}_parent" height="100%" float="right" grow="1" regrow="1" growinvis="0">
			<!-- <instance name="gameVerticalBar" prefix="gameAbility" id="{id}" type="Vertical" height="6h" framewidth="+3h" borderthickness="0.5h"/> -->
			<instance name="gameVerticalBar" prefix="gameAbility" id="{id}" type="Timer" 	height="6h" framewidth="+3h" borderthickness="0.5h"/>

			<!-- Core Ability Stuff -->
			<panel width="6.6h" height="100%" reverse="1" sticky="1" stickytoinvis="0">
				<!-- Top half of the ability with the icon, level up arrow, and ticks -->
				<panel width="100%" height="100@" align="center" valign="top" noclick="1">
					<frame style="generalShadow"/>
					<panel style="gameAbilityIconContainer">
						<image name="gameAbility{id}_icon" texture="{texture}" align="center" valign="center" noclick="0" />
						
						<instance name="iconOverlays" prefix="gameAbility" id="{id}" />
					</panel>
					<frame texture="/ui/shared/frames/rounded_bg_grad.tga" color="invisible" borderthickness="0.5h" bordercolor="#505050" noclick="1">
						<image texture="/ui/shared/textures/mini_button_corner_tl.tga" y="-0.1h" x="-0.1h" height="2.5h" width="2.5h" align="left" valign="top" noclick="1" />
						<image texture="/ui/shared/textures/mini_button_corner_tr.tga" y="-0.1h" x="0.1h" height="2.5h" width="2.5h" align="right" valign="top" noclick="1" />
						<image texture="/ui/shared/textures/mini_button_corner_bl.tga" y="0.1h" x="-0.1h" height="2.5h" width="2.5h" align="left" valign="bottom" noclick="1" />
						<image texture="/ui/shared/textures/mini_button_corner_br.tga" y="0.1h" x="0.1h" height="2.5h" width="2.5h" align="right" valign="bottom" noclick="1" />
						
						<panel y="-40%" width="100%" height="2h" valign="top" noclick="1" >
							<panel width="100%" valign="bottom" float="bottom" grow="1" regrow="1" growinvis="0" noclick="1">
								<!-- Horizontal Timer Bars  -->			
								<frame name="gameAbility{id}ChargeBarContainer" height="0.5h" widgth="100%" sticky="1" stickytoinvis="0">
									<panel style="gameHorizontalBarBase">
										<panel style="gameVerticalBarBlank">
											<panel name="gameAbility{id}ChargeBar" style="gameHorizontalBar" />
										</panel>
									</panel>
								</frame>

								<panel name="gameAbility{id}_tickContainer" width="100%" height="0.7h" sticky="1" stickytoinvis="0" noclick="1">
									<!-- Pips -->
									<panel y="2h" align="center" float="right" grow="1" regrow="1" growinvis="0" noclick="1" >
										<instance name="gameAbilityTick" id="{id}" count="1"/> <!-- For testing - we would want this to just populate as needed -->
										<instance name="gameAbilityTick" id="{id}" count="2"/>
										<instance name="gameAbilityTick" id="{id}" count="3"/>
										<instance name="gameAbilityTick" id="{id}" count="4"/>
										<instance name="gameAbilityTick" id="{id}" count="5"/>
										<instance name="gameAbilityTick" id="{id}" count="6"/>
									</panel>
								</panel>
							</panel>
						</panel>
						
						<animatedimage name="gameAbility{id}_levelUp" align="center" valign="bottom" height="50%" width="50%" color="1 1 1 .8" texture="/ui/game/shared/textures/lvl_up_arrow/lvl_up.tga" loop="1" fps="30" onshow="StartAnim(1);" noclick="1"/>
					</frame>
				</panel>
				
				<!-- Bottom half of the ability with the keybind, ability levels, and pet passive -->
				<panel width="90%" height="3.2h"  align="center" valign="bottom" noclick="1" texture="/ui/shared/textures/decal_base.tga">
					<panel style="gameAbilityInfoContainer">
						<instance id="gameAbility{id}" name="key" keybind="{keybind}" font="maindyn_9" height="105%" width="105@"/>
						<instance name="gameAbility{type}Info" id="{id}" />
					</panel>
				</panel>
			</panel>	
		</panel>
	</template>
	
	<!-- Minimap Templates -->
	<template name="gameMinimapBottom">
		<panel name="gameMinimapBottom{id}" noclick="1">
			<image x="{x}3.2h" y="-4.5" width="3.2h" height="12.8h" texture="/ui/shared/textures/decal_edge.tga" align="{align}" valign="bottom" noclick="1" hflip="{hflip}"/>
