		<style name="gameShopSelectRecipeLabel" 					   height="4h"  								x="1.5h"   y="0.2h" 			 style="gameShopSelectRecipeLabel_base" />
	</if>
	
	<!--
	===========================	
	Includes
	===========================
	-->
	<include file="/ui/shared/templates_elements_simple.package"/>
	<childinterface file="/ui/game/shared/game_shared_templates.interface"/>
	
	<include file="/ui/new_game/shop/shop_templates.package"/>
	
	<!--
	===========================	
	Core Interface
	===========================
	-->
	<panel name="gameShopContainer" style="gameShopContainer" blockinput="true" visible="false" hotkey="ESC" onhotkey="PlaySound('/shared/sounds/ui/button_close_01.wav'); CloseShop();" y="300s"><!-- Shop Container -->
		<!-- Item Bookmarks -->
		<panel name="gameShopBuyQueueContainer" noclick="1" align="right" x="7.1h" y="0.2h" height="1.5h" width="8h">
			<panel y="0.8h" x="0.3h" width="7h" height="68h" align="center" noclick="true">		
		
				<!-- Full Catch Area -->
				<panel name="autoBuyQueueAddDropTargetFull" visible="false">
					<instance name="DropLand" width="100%" height="100%" visible="1" noclick="1"/>
				</panel>				
				
				<panel grow="1" regrow="1" width="100%" float="bottom" y="0" noclick="1">					
					<panel grow="1" regrow="1" width="100%" noclick="1">
						<panel noclick="1" >
							<instance name="shopShadowsThin" />
							<image name="gameShopBuyQueueBackground" texture="/ui/new_game/shop/textures/shop_bg.jpg" width="+0.5h" vtile="1" color="#696969" vscale="128p" noclick="1"
								onevent="SetVScale(GetHeight() # 'p');" onload="DoEvent()"
							/>
							<frame texture="/ui/shared/frames/stone.tga" width="+0.5h" color="invisible" borderthickness="5h" bordercolor="#bcbcbc" noclick="1">
								<image texture="/ui/shared/textures/stone_corner_br.tga" y="0.2h" x="0.2h" height="3h" width="3h" align="right" valign="bottom" color="#bcbcbc" noclick="1" />
							</frame>
							<panel width="+0.5h" float="right" noclick="1">
								<image texture="/ui/main/shared/textures/grad_vert.tga" width="-0.5h" vflip="1" color="0 0.78 1 0.1" rendermode="additive"/>
								<image texture="/ui/main/shared/textures/grad_vert.tga" width="0.5h" vflip="1" color="0 0.78 1 0.2" rendermode="additive"/>
							</panel>
						</panel>
						<panel y="3.5h" grow="1" regrow="1" growinvis="0" float="bottom" padding="0" width="100%" noclick="1">
							<instance name="buyQueueEntry" id="0"/>
							<instance name="buyQueueEntry" id="1"/>
							<instance name="buyQueueEntry" id="2"/>
							<instance name="buyQueueEntry" id="3"/>
							<instance name="buyQueueEntry" id="4"/>
							<instance name="buyQueueEntry" id="5"/>
							<instance name="buyQueueEntry" id="6"/>
							<instance name="buyQueueEntry" id="7"/>
							<instance name="buyQueueEntry" id="8"/>
							<panel name="autoBuyQueueAdd" width="100%" height="100@" noclick="true" sticky="true" stickytoinvis="false">
								<panel width="80%" height="80@" align="center">
									<image name="autoBuyQueueAdd_icon" texture="/ui/shared/shop/textures/scroll_target.tga" color="0.12 0.43 0.8 0.5"/>
									<instance name="DropLand" id="autoBuyQueueAddDropTarget" width="+0.8h" height="+0.8h" />
								</panel>
							</panel>
						</panel>
					</panel>				
				</panel>				
				<image texture="/ui/shared/shop/textures/scroll_highlight.tga" y="0.5h" width="100%" height="15@" noclick="1" color="1 1 1 .1" />
			</panel>			
			<image texture="/ui/shared/shop/textures/scroll_slot.tga" width="100%" height="12.5@" noclick="1" />
		</panel>
		
		<!-- Base of the Shop -->		
		<instance name="shopShadows" />
		<panel name="gameShopLevelUpKeyController" noclick="true" color="invisible"/>
		<frame name="gameShopContainer_dummyglow_2" visible="0" noclick="true" texture="/ui/shared/frames/frameglow.tga" color=".4 .9 1 .7" bordercolor=".4 .9 1 .7" rendermode="additive" width="+26s" height="+26s" align="center" valign="center" borderthickness="18s" />				
		<frame name="gameShopContainer_dummyglow_1" visible="0" noclick="true" color="invisible" bordercolor="invisible" width="+230s" height="+630s" align="center" valign="center">				
			<modelpanel height="+460s" width="+130s" align="center" valign="center" visible="1"
				noclick="true"
				model="/shared/models/invis.mdf"
				effect="/ui/main/crafting/effects/alert_wide.effect"
				onshowlua="self:SetEffect('/ui/main/crafting/effects/alert_wide.effect')"
				onhidelua="self:SetEffect('')"					
				camerapos="0 0 0"
				cameraangles="0 0 0"
				camerafov="70"
				depthclear="true"
				depthcompress="true"
			/>
		</frame>		
		<image texture="/ui/new_game/shop/textures/shop_bg.jpg" noclick="1" vtile="1" vscale="128p"/>
		<frame x="-1h" texture="/ui/elements:rounded_bg_white" width="+1.3h" height="+0.3h" color="invisible" borderthickness="0.3h" bordercolor="#00080d" noclick="1"/>
		
		<!-- Header Bar/Menu -->		
		<label name="gameShopSelectRecipeLabel" style="gameShopSelectRecipeLabel" content="game_shop_select_recipe_to_modify"/>
		<panel name="gameShopHeader" style="gameShopHeader">
			<instance name="shopShadows" />
			<image style="gameShopHeaderBar" />
			
			<!-- Buy Item Button -->
			<panel style="gameShopTabItemsPlacer">			
				<panel style="gameShopTabPlacerPlate" noclick="true">
					<instance name="shopShadowsThin" />
					<frame texture="/ui/shared/frames/rounded_bg_grad.tga" borderthickness="0.4h" color="#535353" noclick="1">
						<button name="gameShopTabItemsButton" interactive="false" color="invisible" />
						<panel style="gameShopTabPlacerPlateMargins" noclick="true">
							<panel width="-72@" y="-0.2h" noclick="true" passivechildren="true">
								<label name="gameShopTabItemsLabel" textalign="center" textvalign="top" font="maindyn_18" outline="true" outlinecolor="0 0 0 .6" color="1 1 1 1" content="game_shop_label_buyitems"/>
								
								<!-- Gold -->
								<panel grow="1" regrow="1" y="0.1h" x="0.5h" height="1.9h" valign="bottom" float="right" padding="0.4h" align="center" noclick="1">
									<label name="gameShopHeroGold" fitx="true" style="labelBase" color="#eda00f" outline="1" outlinecolor="#1c1c1c" textvalign="center" font="maindyn_14" content="" noclick="1"/>
									<panel width="100@" height="100%" noclick="1">
										<image texture="/ui/elements:gold_coins" y="0.2h" width="100@" height="100%" noclick="1"/>
									</panel>
								</panel>
								
							</panel>
							
							<!-- Hotkey Button -->
							<instance y="7%" x="7@" name="shopShadowsThin" width="100@" height="100%" align="right" />
							<instance id="shopItemListTabHotkey" name="shopBindableHotkeyButton" align="right" width="85@" height="85%" valign="center"
								labelY="-0.15h"
								onrefreshlua="
									local button = GetKeybindButton('game', 'ToggleShop', '')
									if button then
										self:SetText(button)
									else
										self:SetText('')
									end
								"
							/>
							
						</panel>
					</frame>
				</panel>			
			</panel>
			
			<!-- Ability Button -->
			<panel style="gameShopTabAbilitiesPlacer">
				<panel style="gameShopTabPlacerPlate">
					<instance name="shopShadowsThin" />
					<frame texture="/ui/shared/frames/rounded_bg_grad.tga" borderthickness="0.4h" color="#535353" noclick="1">
						<button name="gameShopTabAbilitiesButton" interactive="false" color="invisible" />
						<panel style="gameShopTabPlacerPlateMargins" noclick="true">
							<panel width="-72@" y="-0.2h" noclick="true" passivechildren="true">
								<label name="gameShopTabAbilitiesLabel" textalign="center" textvalign="top" font="maindyn_18" outline="true" outlinecolor="0 0 0 .6" color="1 1 1 1" content="game_shop_label_abilities"/>
								
								<!-- Ability Points -->
								<panel grow="1" regrow="1" y="0.1h" x="0.5h" height="1.9h" valign="bottom" float="right" padding="0.4h" align="center" noclick="1">
									<label fitx="true" style="labelBase" color=".7 .7 .7 1" textvalign="center" font="maindyn_14" content="game_shop_label_skillpoints" noclick="1"/>
									<label name="gameShopTabAbilitiesLabel_points" fitx="true" style="labelBase" color=".7 .7 .7 1" textvalign="center" font="maindyn_14" content="" noclick="1"
										onloadlua="
											self:UnregisterWatchLua('ActiveUnit')
											self:RegisterWatchLua('ActiveUnit', function(widget, trigger)
												widget:SetText(trigger.availablePoints)
											end, false, nil, 'availablePoints')									
										"
									/>								
								</panel>
								
							</panel>

							<!-- Hotkey Button	-->
							<instance y="7%" x="7@" name="shopShadowsThin" width="100@" height="100%" align="right" />
							<instance id="shopAbilitiesTabHotkey" name="shopBindableHotkeyButton" align="right" width="85@" height="85%" valign="center"
								labelY="-0.15h"
								onrefreshlua="
									local button = GetKeybindButton('game', 'TriggerToggle', 'gameShowSkills')
									if button then
										self:SetText(button)
									else
										self:SetText('')
									end
								"
							/>
							
						</panel>
					</frame>
				</panel>
			</panel>	

			<!-- Close Shop Button -->
			<instance name="framed_mini_button" id="game_shop_closex" x="-2.4h" y="-0.2h" width="3.5h" height="3.5h" align="right" valign="center" bindShow="false" texture="/ui/shared/textures/mini_close.tga" state="2" iconcolor="#ffc5c5"/>
		</panel>
		
		<!-- Load Button -->
		<panel x="10.5h" y="-2h" align="right" width="14h" height="7h" noclick="1" texture="/ui/new_game/shop/textures/shop_load.tga">
			<instance id="gameShopBookmarkIcon"
					y="0.5h" x="0.2h"
					width="50%" height="3h"
					align="center" valign="center" labelY="-0.1h"
					borderthickness="0.4h"
					name="standardButton"
					label="general_load_cap"
					font="maindyn_14"
					outlinecolor="#75130a"
				/>
		</panel>

		<!-- Item Categories -->
		<panel name="gameShopCategoryList" style="gameShopCategoryList">
			<!-- Top half of the frame -->
			<image texture="/ui/new_game/shop/textures/shop_bg.jpg" y="-0.7h" height="7h" vtile="1" color="#696969" vscale="128p" noclick="1"/>			
			<frame texture="/ui/shared/frames/stone.tga" y="-0.7h" height="7.2h" color="invisible" borderthickness="3h" bordercolor="#ababab" noclick="1">
				<image texture="/ui/shared/textures/stone_corner_tl.tga" y="-0.2h" x="-0.2h" height="3h" width="3h" align="left"  valign="top" color="#ababab" noclick="1" />
				<image texture="/ui/shared/textures/stone_corner_tr.tga" y="-0.2h" x="0.2h" height="3h" width="3h" align="right" valign="top" color="#ababab" noclick="1" />
				<image texture="/ui/shared/textures/stone_corner_bl.tga" y="0.2h" x="-0.2h" height="3h" width="3h" align="left"  valign="bottom" color="#ababab" noclick="1" />
				<image texture="/ui/shared/textures/stone_corner_br.tga" y="0.2h" x="0.2h" height="3h" width="3h" align="right" valign="bottom" color="#ababab" noclick="1" />
			</frame>
			
			<!-- Bottom half of the frame -->
			<frame texture="/ui/shared/frames/stone.tga" y="6.8h" height="5.5h" color="invisible" borderthickness="3h" bordercolor="white" noclick="1">
				<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.6h" color="0 0.55 0.73 0.2" noclick="1"/>
			</frame>
			
			<panel style="gameShopCategoryRow1" >
				<instance name="shopCategoryButton" id="2" icon="recommended"/>
				<instance name="shopCategoryButton" id="5" icon="caster"/>
				<instance name="shopCategoryButton" id="4" icon="physical"/>
				<instance name="shopCategoryButton" id="6" icon="mana"/>
				<instance name="shopCategoryButton" id="7" icon="defense"/>
				<instance name="shopCategoryButton" id="8" icon="support"/>
				<instance name="shopCategoryButton" id="3" icon="boots"/>
			</panel>

			<!-- Second Row -->
			<panel style="gameShopCategoryRow2">

				<!-- All/Search Box -->
				<panel style="gameShopSearch">
					<instance name="shopCategoryButton2" id="1" icon="all"/>
					
					<panel style="gameShopSearchBar">
						<panel align="right" valign="center" height="90%" noclick="1">
							<frame name="game_shop_search_bg" texture="/ui/shared/frames/text_input.tga" borderthickness="0.6h" color="0.9 0.9 0.9 1" noclick="1"/>
							<label name="game_shop_search_cover_label" style="game_shop_search_cover_label" content="game_shop_search"/>
							<textbox name="game_shop_search_input" style="game_shop_search_input" 
								onesc="
									EraseInputLine()
								"
								onfocuslua="
									trigger_gamePanelInfo.selectedShopItem = -1
									trigger_gamePanelInfo.selectedShopItemType = ''
									trigger_gamePanelInfo:Trigger(false)

									trigger_shopFilter.shopCategory	= ''
									trigger_shopFilter:Trigger(false)

									self:GetWidget('game_shop_search_cover_label'):FadeOut(50)
									self:GetWidget('game_shop_search_closex'):FadeIn(50)
									self:GetWidget('game_shop_search_bg'):SetColor('1 1 1 1')
									self:GetWidget('game_shop_search_bg'):SetBorderColor('1 1 1 1')
								"
								onenter="DoEvent()"
								onevent="self:SetFocus(false)"
								onchangelua="
									if (string.len(tostring(self:GetValue())) &#62; 0) then
										self:GetWidget('game_shop_search_cover_label'):FadeOut(50)
										self:GetWidget('game_shop_search_closex'):FadeIn(50)
										self:GetWidget('game_shop_search_bg'):SetColor('1 1 1 1')
										self:GetWidget('game_shop_search_bg'):SetBorderColor('1 1 1 1')
										Shop.SetSearchString(self:GetValue())
									else
										self:GetWidget('game_shop_search_cover_label'):FadeIn(250)
										self:GetWidget('game_shop_search_closex'):FadeOut(250)
										self:GetWidget('game_shop_search_bg'):SetColor('0.9 0.9 0.9 1')
										self:GetWidget('game_shop_search_bg'):SetBorderColor('0.9 0.9 0.9 1')
										Shop.SetSearchString('')
									end
								"
							/>
							<instance name="closeXButton" buttonName="game_shop_search_closex" width="80@" height="80%" align="right" x="-15@" valign="center"
