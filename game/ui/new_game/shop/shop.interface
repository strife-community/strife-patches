		<style name="gameShopCategoryRow2"  						   height="4h" 											   y="0.2h"  			 padding="1h" style="gameShopCategoryRow2_base" />
		
		<style name="gameShopSearch" 								   																				 padding="1h" style="gameShopSearch_base" />
		<style name="gameShopSearchBar" 				width="22h"																	     noclick="1" />		
		<style name="game_shop_search_cover_label" 												   				    x="1.2h"   y="-0.1h" 			 style="game_shop_search_cover_label_base" /> 
		<style name="game_shop_search_input" 																				   y="0.3h" 			 hmargin="0.65h" vmargin="0.25h" style="game_shop_search_input_base"/>
		<style name="shopCategoryButtonSep" 			width="1" 																		 noclick="1" />
		<style name="shopDisplayOptions" 							   																				 padding="1.0h" style="shopDisplayOptions_base" />
		<style name="shopDisplayOptionsSep" 			width="1.0h" 																	 noclick="1" />
