		<style name="gameShopCategoryList" 							   height="106s" 										   						 style="gameShopCategoryList_base" />
		<style name="gameShopCategoryRow1" 							   height="60s"  				 				x="-12s" 			 			 padding="10s" style="gameShopCategoryRow1_base" />
		<style name="gameShopCategoryRow2" 							   height="40s" 			   							 						 padding="10s" style="gameShopCategoryRow2_base" />
		
