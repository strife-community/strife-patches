		<style name="gameShopItemListItemName" 	   					   height="1.5h" 																 style="gameShopItemListItemName_base" />	
		
		<style name="gameShopContainer" 		   		width="52.9h"  height="-14h" align="left"  					x="-52.9h" 		  />
		<style name="gameShopCategoryList" 							   height="11.2h" 										   						 style="gameShopCategoryList_base" />
