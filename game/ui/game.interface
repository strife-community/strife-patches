				if (cameraModeTrigger.camMode == 0) then
					Cmd('Action ToggleLockedCam')
				else
				
				end		
		"
		onloadlua="
			local npeTrigger = LuaTrigger.GetTrigger('newPlayerExperience')
			if (mainUI) and (mainUI.savedRemotely) and (not mainUI.savedRemotely.hasSelectedCameraMode) and (npeTrigger) and (npeTrigger.tutorialProgress >= NPE_PROGRESS_TUTORIALCOMPLETE) then
				mainUI.savedRemotely.hasSelectedCameraMode = true
				SaveState()
				self:SetVisible(1)
			end
		"
	>	
		<?lua
			object:RegisterWatchLua('GameReinitialize', function(widget, trigger)
				local npeTrigger = LuaTrigger.GetTrigger('newPlayerExperience')
				if (mainUI) and (mainUI.savedRemotely) and (not mainUI.savedRemotely.hasSelectedCameraMode) and (npeTrigger) and (NPE_PROGRESS_TUTORIALCOMPLETE <= npeTrigger.tutorialProgress) then
					mainUI.savedRemotely.hasSelectedCameraMode = true
					SaveState()
					widget:SetVisible(1)
				end
			end)
			object:RegisterWatchLua('GamePhase', function(widget, trigger)
				local npeTrigger = LuaTrigger.GetTrigger('newPlayerExperience')
				if (trigger.gamePhase == 5) and (mainUI) and (mainUI.savedRemotely) and (not mainUI.savedRemotely.hasSelectedCameraMode) and (npeTrigger) and (NPE_PROGRESS_TUTORIALCOMPLETE <= npeTrigger.tutorialProgress) then
					mainUI.savedRemotely.hasSelectedCameraMode = true
					SaveState()
					widget:SetVisible(1)
				end
			end)			
		?>
