	<!-- Standard Button -->
	<template name="standardButtonState_game">
		<widgetstate statename="{statename=up}" x="{x}" y="{y}">
			<frame texture="{texture=/ui/shared/frames/stnd_btn_up.tga}" borderthickness="{borderthickness}" color="{color=1 1 1 1}" rendermode="{rendermode}"/>
			<label x="{labelX=0}" group="{id}_label_group" y="{labelY=-0.2h}" textalign="center" textvalign="center" style="labelBase" font="{font=maindyn_16}" color="{labelcolor=white}" content="{label=temp_button}"/>
		</widgetstate>
	</template>
	<template name="standardButton_game">
		<button name="{id}" width="{width=100%}" height="{height=100%}" x="{x}" y="{y}" align="{align}" valign="{valign}" passivechildren="true" onloadlua="{onloadlua}" visible="{visible=true}" sticky="{sticky}" stickytoinvis="{stickytoinvis=true}" interactive="{interactive=false}" enabled="{enabled=true}" disabledmouseover="{disabledmouseover=false}" disabledmouseout="{disabledmouseout=false}" noclick="{noclick=false}" color="invisible" onclick="{onclick}" onclicklua="{onclicklua}"	>
			<instance name="standardButtonState_game" id="{id}" font="{font=maindyn_16}" labelX="{labelX=0}" label="{label=temp_button}" rendermode="{rendermode=normal}" borderthickness="{borderthickness=0.6h}" statename="up"/>
			<instance name="standardButtonState_game" id="{id}" font="{font=maindyn_16}" labelX="{labelX=0}" label="{label=temp_button}" rendermode="{rendermode=normal}" borderthickness="{borderthickness=0.6h}" texture="/ui/shared/frames/stnd_btn_over.tga" statename="over"/>
			<instance name="standardButtonState_game" id="{id}" font="{font=maindyn_16}" labelX="{labelX=0}" label="{label=temp_button}" rendermode="{rendermode=normal}" borderthickness="{borderthickness=0.6h}" statename="down" x="1" y="1"/>
			<instance name="standardButtonState_game" id="{id}" font="{font=maindyn_16}" labelX="{labelX=0}" label="{label=temp_button}" rendermode="grayscale" borderthickness="{borderthickness=0.6h}" color=".8 .8 .8 1" labelcolor="#777777" statename="disabled"/>
		</button>
	</template>

	<template name="gameBindableHotkeyButton">
		<panel name="{id}" style="{style}" align="{align}" valign="{valign}" width="{width=100%}" height="{height=100%}" x="{x=0h}" y="{y=0h}" noclick="true">
			<panel name="{id}Body" align="center" valign="center" noclick="true" passivechildren="true">
				<frame name="{id}Backer" texture="/ui/elements:key" visible="1" borderthickness="{borderthickness=33@}"/>
				<frame name="{id}Highlight" texture="/ui/elements:rounded_bg_white" rendermode="additive" color="#FFCC0011"/>
				<label name="{id}Label" style="gameHotkeyLabel" font="{font=maindyn_14}" content="-" y="{labelY}"
					onshowlua="{onrefreshlua}"
					onrefreshlua="{onrefreshlua}"
					oneventlua="{oneventlua}"
				/>
			</panel>
			<button name="{id}ButtonTip" hmargin="5@" vmargin="5%" noclick="false" interactive="false" color="invisible"/>
			<button name="{id}Button" hmargin="5@" vmargin="5%" noclick="true" interactive="false" color="invisible"
				onloadlua="libButton2.initializeButton(self, '{id}', 'key')"
			/>
		</panel>
	</template>

	<template name="pinButton">
		<button name="{id}" color="invisible" width="{width=100%}" height="{height=100%}" x="{x}" y="{y}" align="{align}" valign="{valign}" visible="{visible=true}" sticky="{sticky}" stickytoinvis="{stickytoinvis}" interactive="{interactive=false}"
			onloadlua="libButton2.initializeButton(self, '{id}', 'basicBounce')"
		>
			<panel align="center" valign="center" name="{id}Body">
				<frame texture="/ui/elements:rounded_button_bg_white" borderthickness="0.35h" color="#444444"/>
				<image name="{id}PinFlair" align="center" valign="center" width="-25@" height="-25%" texture="/ui/elements:pulse" color="#00BFFF" visible="0"/>
				<image name="{id}Pin" align="center" valign="center" width="-25@" height="-25%" texture="/ui/elements:pin" color="#00BFFF"/>
			</panel>
		</button>
	</template>

	<template name="simpleButtonKey">
		<panel name="{id}Container" width="{width=100%}" height="{height=100%}" x="{x}" y="{y}" visible="{visible=1}" align="{align}" valign="{valign}" onloadlua="{onloadlua}" noclick="true" sticky="{sticky}" stickytoinvis="{stickytoinvis=true}" interactive="{interactive=false}">
			<button color="invisible" visible="{visible=true}" name="{id}" interactive="false"
				onloadlua="libButton2.initializeButton(self, '{id}', 'standardButton2')"
			/>
			
			<panel name="{id}Body" align="center" valign="center" noclick="true" passivechildren="true">
				<frame name="{id}Background" texture="/ui/shared/frames/stnd_btn_up.tga" borderthickness="{borderthickness=0.6h}"/>
				<label name="{id}Label" width="-90@" height="100%" x="{labelX}" y="{labelY=-0.2h}" textalign="center" textvalign="center" style="labelBase" font="{font=maindyn_18}" color="{labelcolor=white}" content="{label=temp_button}"/>
			</panel>
			
			<instance id="{id}Key" name="gameBindableHotkeyButton" align="right" width="90@" height="85%" valign="center" x="-0.35h"
				font="{keyFont}"
				onrefreshlua="{onrefreshlua}"
				oneventlua="{onrefreshlua}"
			/>
		</panel>
	</template>

