						<frame name="game_shop_search_bg" texture="/ui/shared/frames/text_input.tga" borderthickness="0.6h" color=".8 .8 .8 1.0" noclick="1"/>
						<label name="game_shop_search_cover_label" style="game_shop_search_cover_label" content="game_shop_search"/>
						<textbox name="game_shop_search_input" style="game_shop_search_input" 
							onesc="
								EraseInputLine()
							"

							onfocuslua="
								trigger_gamePanelInfo.selectedShopItem = -1
								trigger_gamePanelInfo.selectedShopItemType = ''
								trigger_gamePanelInfo:Trigger(false)

								trigger_shopFilter.shopCategory	= ''
								trigger_shopFilter:Trigger(false)

								self:GetWidget('game_shop_search_cover_label'):FadeOut(50)
								self:GetWidget('game_shop_search_closex'):FadeIn(50)
								self:GetWidget('game_shop_search_bg'):SetColor('1 1 1 1')
								self:GetWidget('game_shop_search_bg'):SetBorderColor('1 1 1 1')
							"
							onenter="DoEvent()"
							onevent="self:SetFocus(false)"
							onchangelua="
								if (string.len(tostring(self:GetValue())) &#62; 0) then
									self:GetWidget('game_shop_search_cover_label'):FadeOut(50)
									self:GetWidget('game_shop_search_closex'):FadeIn(50)
									self:GetWidget('game_shop_search_bg'):SetColor('1 1 1 1')
									self:GetWidget('game_shop_search_bg'):SetBorderColor('1 1 1 1')
									Shop.SetSearchString(self:GetValue())
								else
									self:GetWidget('game_shop_search_cover_label'):FadeIn(250)
									self:GetWidget('game_shop_search_closex'):FadeOut(250)
									self:GetWidget('game_shop_search_bg'):SetColor('.8 .8 .8 1')
									self:GetWidget('game_shop_search_bg'):SetBorderColor('.8 .8 .8 1')
									Shop.SetSearchString('')
								end
							"
						/>
						<instance name="closeXButton" buttonName="game_shop_search_closex" width="80@" height="80%" align="right" x="-15@" valign="center"
