					libGeneral.createGroupTrigger('ShopItemExistance', { 'ShopItem0.exists', 'ShopItem1.exists', 'ShopItem2.exists', 'ShopItem3.exists', 'ShopItem4.exists', 'ShopItem5.exists' })
					object:RegisterWatchLua('ShopItemExistance', function(widget, groupTrigger)
						local showIt = true
						for i = 0,5,1 do
