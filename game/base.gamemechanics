	<targetscheme name="all" allow="all" restrict=""/>	
	<targetscheme name="all_heroes" allow="hero" restrict="dead,corpse"/>
	<targetscheme name="all_heroes_dead_alive" allow="hero" restrict=""/>
	<targetscheme name="shopkeeper" allow="unit" restrict="!ally,hero,dead,corpse,tree,gadget,chest,Courier,PseudoPet,AntiTP,MedicPet,Unit_Spellblade,!shopkeeper"/>
