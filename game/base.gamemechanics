	<targetscheme name="tutorial_hero2" allow="hero" restrict="ally,dead,corpse,!Hero_CapriceTutorial2"/>
	<targetscheme name="teleporter" allow="Gadget_Tutorial_Boss_Home" restrict="!Gadget_Tutorial_Boss_Home"/>
	<targetscheme name="Gadget_Tutorial_NoCast" allow="gadget" restrict="hero,dead,corpse,tree,chest,Courier,PseudoPet,AntiTP,MedicPet,Unit_Spellblade,!Gadget_Tutorial_NoCast"/>
	<targetscheme name="Gadget_Tutorial2_NoCast" allow="gadget" restrict="hero,dead,corpse,tree,chest,Courier,PseudoPet,AntiTP,MedicPet,Unit_Spellblade,!Gadget_Tutorial2_NoCast"/>
